    "@reskript/cli": "5.2.1",
    "@reskript/cli-build": "5.2.1",
    "@reskript/cli-dev": "5.2.1",
    "@reskript/cli-lint": "5.2.1",
    "@reskript/config-lint": "5.2.1",
    "@reskript/settings": "5.2.1",
    "@testing-library/react-hooks": "^7.0.2",
