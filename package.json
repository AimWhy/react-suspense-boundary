    "antd": "^4.19.3",
    "c8": "^7.11.0",
    "core-js": "^3.21.1",
    "echarts": "^5.3.2",
    "eslint": "^8.12.0",
    "history": "^5.3.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "prism-themes": "^1.9.0",
    "react": "^18.0.0",
